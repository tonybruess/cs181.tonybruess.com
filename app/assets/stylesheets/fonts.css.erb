@font-face {
    font-family: 'isocpeur';
    src: url('<%= asset_path('isocpeur.eot') %>');
    src: url('<%= asset_path('isocpeur.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('isocpeur.woff') %>') format('woff'),
         url('<%= asset_path('isocpeur.ttf') %>') format('truetype'),
         url('<%= asset_path('isocpeur.svg') %>') format('svg');
    font-weight: normal;
    font-style: normal;
}
